@charset "utf-8";
/* CSS Document */

/*==================================   20170619 巴中数字民政————公墓管理 样式  ========================================================*/


.cemetery {}


.cemetery .s-skin-container-default {background:url("../images/bgb01_bzgmgl.jpg") no-repeat center; background-size:cover;}

.cemetery .m-area-left {/*left:40px;*/ left:5%; bottom:30px;}
.cemetery .m-area-right {right:40px; bottom:30px; width:31%;}


.m-glgls-tit {position:relative; z-index:105; width:100%; height:50px; font-size:1.5rem; line-height:50px; color:#fff; overflow:hidden;}

.cemetery .tj-boxes {position:relative; /*width:440px; height:300px;*/ width:100%; height:auto!important; margin-bottom:10%; z-index:102; zoom:1; overflow:hidden;}

.m-glgls-main {font-size:1.3rem; color:rgba(255,255,255,.7);}
.m-glgls-main span {display:inline-block;}
.m-glgls-main .num {font-size:3.75rem; font-family:"Let's go Digital" , "Arial", "Helvetica", "sans-serif"; margin-right:3%;}

.cemetery .orange {color:#ea8117;}
.cemetery .green {color:#17e739;}
.cemetery .blue {color:#2ea8e5;}
.cemetery .red {color:#ff2426;}

.mkh {width:9px; height:17px;}
.khll {background:url("../images/bzgmgl_mkh01.png") no-repeat; margin-right:3%;}
.khrr {background:url("../images/bzgmgl_mkh02.png") no-repeat; margin-left:3%;}


.m-area-bottom {position:absolute; left:40px; bottom:30px; width:63%; height:36%;}

.m-gm-boxes {position:relative; width:100%; height:100%; z-index:102; zoom:1; color:#fff; overflow:hidden;}
.you01 {height:48%; margin-bottom:25px;}
.you02 {height:49%;}

.m-gm-bj-b {width:100%; height:10%; background:url(../images/bzgmgl_bj_b.png) no-repeat center bottom; background-size:100%;}
.m-gm-main {width:100%; height:78%; background:url(../images/bzgmgl_bj_m.png) repeat-y center top; background-size:100%; position:relative; padding-top:1.0em;}
.gmgl-tit {/*position:absolute; left:0; top:-3%;z-index:103;*/width:100%; height:2em; text-align:center; color:#fff21a; font-size:1.5rem; background:url(../images/bzgmgl_bj_t.png) no-repeat center 9px; background-size:100%;}

.m-area-bottom .gmgl-tit {background:url(../images/bzgmgl_bj_tb.png) no-repeat center 9px; background-size:100%;}
.m-area-bottom .m-gm-bj-b {width:100%; height:10%; background:url(../images/bzgmgl_bj_bb.png) no-repeat center bottom; background-size:100%;}
.m-area-bottom .m-gm-main {height:76%; background:url(../images/bzgmgl_bj_mb.png) repeat-y center top; background-size:100%;}


.glgl-con {margin:0 auto; width:88%;}
.glgl-con table {font-size:1.02rem;}
.glgl-con th {font-weight:normal; height:50px; background:rgba(255,242,26,.2)}
.glgl-con td {text-align:center; padding:0.3em 0;}
.glgl-con .orange , .glgl-con .blue , .glgl-con .red {font-size:1.5rem;}



/*各分辨率兼容性*/

