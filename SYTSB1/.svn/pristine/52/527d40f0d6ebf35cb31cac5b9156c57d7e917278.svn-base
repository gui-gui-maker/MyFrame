@charset "utf-8";
/* CSS Document */


/*==================================   全局样式  ========================================================*/

/** {-moz-box-sizing:border-box; -ms-box-sizing:border-box; box-sizing:border-box;}*/
html, body {width: 100%; height:100%;}
/*img {border:0; vertical-align:middle;}*/
/*body {font-size:14px; font-family: "Microsoft Yahei", "黑体", "宋体"; overflow: hidden; color:#fff; background:#000 url(../img/timg.jpg) no-repeat;}*/
em {font-style:normal;}
a {text-decoration:none; outline:none;}
* :hover , * :active , * :focus {outline:none;}
.clear {clear:both; *display:inline;/*IE only*/}




.page_main {position:relative; z-index: 999;-moz-box-sizing:border-box; -ms-box-sizing:border-box; box-sizing:border-box; height: 100%;}


.side_left {position:fixed; right:0; top:0; bottom:0; height:162px; z-index: 99;}
.side_left2 {position:fixed; right:0; top:0; bottom:0; height:81px; z-index: 99;}

.mnbgone {position:absolute; left:0; top:0; width:100%; height:100%; background:rgba(50, 65, 90,0.75); background:#32415a\0; opacity:0.75\0; filter:alpha(opacity=75)\0; filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=75)\0; z-index:1;}



/*menu --------------------------------------------------------------*/
#menu {margin:0px auto; display:block; /*width:1000px; height:34px;*/ position:relative; z-index:999;}
#menu ul, #menu li {list-style:none; margin:0; padding:0;}

#nav {display:block;}
#nav .mainlevel {/*float:left; background:#3f240e;*/ text-align:center; display:block; border-bottom:1px solid #95bbdc;}
#nav .mainlevel a {color:#fff; line-height:35px; width:100px; height:80px; padding-top:20px; padding-bottom:20px; text-align:center; display:block;-moz-box-sizing:border-box; -ms-box-sizing:border-box; box-sizing:border-box;}
#nav .mainlevel a:hover {text-decoration:none; background:#d8ac03;}
#nav .mainlevel a.active {background:#FB9337;}
#nav .mainlevel .sub_nav_01 {position:absolute; display:none;/*IE is great need, width>=li.length*/ left:100px; top:0; bottom:0; width:200px; height:100%;}
#nav .mainlevel li {/*float:left; background:#3f240e;*/}

#nav .mainlevel .sub_nav_01 ul {position:relative; z-index:6;}
#nav .mainlevel .sub_nav_01 li a {width:200px; line-height:20px; height:50px; display:block; padding:15px 0 15px 30px; text-align:left;}
#nav .mainlevel .sub_nav_01 li a:hover {color:#084b89; text-decoration:none; background:#fff;}
#nav .mainlevel .sub_nav_01 li a:hover .ia {background:#fff url(../img/iconb.png) no-repeat center top;}
#nav li a em {display:block;/* position:absolute; left:0; top:20px;*/ width:100px; height:20px;}

.i1 {background:url(../img/icon1.png) no-repeat center top;}
.i2 {background:url(../img/icon2.png) no-repeat center top;}
.ia {background:url(../img/icona.png) no-repeat center top;}

#nav .mainlevel .sub_nav_01 li a em {display:block; float:left;/* position:absolute; left:0; top:20px;*/ width:20px; height:20px; margin-right:20px;}

.subgtwo {position:absolute; left:0; top:0; width:100%; height:100%; background:rgba(62, 82, 113,0.8); background:#3e5271\9; opacity:0.75\0; filter:alpha(opacity=75)\0; filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=75)\0; z-index:1;}
.layerTitle{position:absolute; z-index: 99;left: 110px; top: 10px; white-space: nowrap; color: #fff; background: #084b89; padding: 2px 5px; border-radius: 5px;filter:alpha(Opacity=80);-moz-opacity:0.7; opacity: 0.7; font-size: 16px;}

.conftop{color:#fff;position: absolute;left: 0px;bottom: 0px;z-index: 999; width: 100px;height:24px;}
.conftop:hover .styleMenu{display:block;}
.styleConf{color:#fff;position: absolute;left: 0px;bottom: 0px; width: 100px; font-size: 14px; border-top:1px solid #95bbdc;text-align: center;}
.m-top-set {float:left; width:100%; height:24px;margin: 6px 0; color:#fff; font-size:24px!important; line-height:24px; -ms-line-height:26px; text-align:center; cursor:pointer; transition:all .3s;}
.m-top-set:hover {transform:rotate(360deg); -webkit-transform:rotate(360deg); -moz-transform:rotate(360deg); color:#f38605; transform-origin:center center;}

.styleMenu{position:absolute; display:none; bottom:1px;left:100px;width:368px;z-index: 999; height:180px; background:rgba(8,75,137,0.7);}

.m-a-boxes {position:relative; width:49%;height:180px;float: left; margin-bottom:5px; z-index:102; zoom:1; overflow:hidden;border-left:solid #ffffff 1px;}
.m-a-boxes-tit {position:relative; z-index:105; padding:0 20px; height:40px; margin-bottom:10px; line-height:40px; color:#fff; background:url(../img/mboxbgline.png) no-repeat center bottom; background-size:100%; overflow:hidden;cursor: pointer;}
.m-a-boxes-tit span {position:absolute; right:20px; top:0px; z-index: 999999999; cursor: pointer;}
.m-a-main {position:relative; z-index:1060; width:163px; padding:0 10px;/*top:48px;bottom:0*/;border:0px solid #FF0000;}
.mapset {}
.mapset dl , .mapset dt , .mapset dd {margin:0; padding:0;}
.mapset dl {float:left; width:153px; line-height:25px; margin-bottom:7px;}
.mapset dt {float:left; width:70px; text-align:right; margin-right:2px;}
.mapset dd {float:left;height: 25px;width: 81px;}
.mapset select {width:81px; height:25px; line-height:22px\9; border:1px solid #ccc; background:#fff;border-radius: 3px;}
.mapset select option {width: 81px;}
.mapset input {height:25px; line-height:25px\9; border:1px solid #ccc; background:#fff;border-radius: 3px;}
.mapset ul{width:153px;}
.mapset ul li{position:relative;float:left;width: 33%;}
.mapset ul li a{color: #ffffff;line-height: 25px;}

.shrinkLeft{position:fixed; left:100px;top:0;width:15px; height:100%; z-index:9; /*background: rgba(46,146,236,0.4);*/ 
background:-webkit-linear-gradient(to right, rgba(46,146,236,0.4), rgba(46,146,236,0));
background:-moz-linear-gradient(to right, rgba(46,146,236,0.4), rgba(46,146,236,0));
background:-o-linear-gradient(to right, rgba(46,146,236,0.4), rgba(46,146,236,0));
background:-webkit-gradient(linear, 0 0, 100% 0, from(rgba(46,146,236,0.4)), to(rgba(46,146,236,0)));
background:linear-gradient(to right, rgba(46,146,236,0.4), rgba(46,146,236,0));opacity: 0; /*transition:auto 0.5s;*/}
.shrinkLeft a{display:block;background:url(../img/sbg.png) no-repeat left center; width: 11px; height:100%; z-index: 99; position: relative;}
.shrinkLeft:hover{opacity: 1;}

.loader{position: absolute;top:0;left:0;width:100%;height:100%;background-color:rgba(8,75,137,0.5);display: none;}
.loader .load-center{position: absolute;left: 50%; top: 45%;}
.loader .load-center .load-txt{position:relative;top: 64px;left: -60px;font-size: 16px;color: #fff;}
@-webkit-keyframes ball-spin-fade-loader {
  50% {
    opacity: 0.3;
    -webkit-transform: scale(0.4);
            transform: scale(0.4); }

  100% {
    opacity: 1;
    -webkit-transform: scale(1);
            transform: scale(1); } }

@keyframes ball-spin-fade-loader {
  50% {
    opacity: 0.3;
    -webkit-transform: scale(0.4);
            transform: scale(0.4); }

  100% {
    opacity: 1;
    -webkit-transform: scale(1);
            transform: scale(1); } }

.ball-spin-fade-loader {
  position: relative; }
  .ball-spin-fade-loader > div:nth-child(1) {
    top: 25px;
    left: 0;
    -webkit-animation: ball-spin-fade-loader 1s 0s infinite linear;
            animation: ball-spin-fade-loader 1s 0s infinite linear; }
  .ball-spin-fade-loader > div:nth-child(2) {
    top: 17.04545px;
    left: 17.04545px;
    -webkit-animation: ball-spin-fade-loader 1s 0.12s infinite linear;
            animation: ball-spin-fade-loader 1s 0.12s infinite linear; }
  .ball-spin-fade-loader > div:nth-child(3) {
    top: 0;
    left: 25px;
    -webkit-animation: ball-spin-fade-loader 1s 0.24s infinite linear;
            animation: ball-spin-fade-loader 1s 0.24s infinite linear; }
  .ball-spin-fade-loader > div:nth-child(4) {
    top: -17.04545px;
    left: 17.04545px;
    -webkit-animation: ball-spin-fade-loader 1s 0.36s infinite linear;
            animation: ball-spin-fade-loader 1s 0.36s infinite linear; }
  .ball-spin-fade-loader > div:nth-child(5) {
    top: -25px;
    left: 0;
    -webkit-animation: ball-spin-fade-loader 1s 0.48s infinite linear;
            animation: ball-spin-fade-loader 1s 0.48s infinite linear; }
  .ball-spin-fade-loader > div:nth-child(6) {
    top: -17.04545px;
    left: -17.04545px;
    -webkit-animation: ball-spin-fade-loader 1s 0.6s infinite linear;
            animation: ball-spin-fade-loader 1s 0.6s infinite linear; }
  .ball-spin-fade-loader > div:nth-child(7) {
    top: 0;
    left: -25px;
    -webkit-animation: ball-spin-fade-loader 1s 0.72s infinite linear;
            animation: ball-spin-fade-loader 1s 0.72s infinite linear; }
  .ball-spin-fade-loader > div:nth-child(8) {
    top: 17.04545px;
    left: -17.04545px;
    -webkit-animation: ball-spin-fade-loader 1s 0.84s infinite linear;
            animation: ball-spin-fade-loader 1s 0.84s infinite linear; }
  .ball-spin-fade-loader > div {
    background-color: #fff;
    width: 15px;
    height: 15px;
    border-radius: 100%;
    margin: 2px;
    -webkit-animation-fill-mode: both;
            animation-fill-mode: both;
    position: absolute; }