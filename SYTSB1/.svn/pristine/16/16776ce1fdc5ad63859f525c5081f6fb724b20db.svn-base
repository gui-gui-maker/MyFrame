package com.lsts.relevant.bean;

/**
 * 特种设备从业人员证书预警数据传输对象 
 * RelevantPeopleDTO
 * 
 * @author GaoYa
 * @date 2014-04-18 上午08:48:00
 */
public class RelevantPeopleDTO {

	// Fields
	private String people_name;
	private String work_com_name;
	private String fk_people_id;
	private float warn_days;

	// Constructors

	/** default constructor */
	public RelevantPeopleDTO() {
	}

	/** full constructor */
	public RelevantPeopleDTO(String people_name, String work_com_name, String fk_people_id, float warn_days) {
		this.people_name = people_name;
		this.work_com_name = work_com_name;
		this.fk_people_id = fk_people_id;
		this.warn_days = warn_days;
	}

	public String getPeople_name() {
		return people_name;
	}

	public void setPeople_name(String people_name) {
		this.people_name = people_name;
	}

	public String getWork_com_name() {
		return work_com_name;
	}

	public void setWork_com_name(String work_com_name) {
		this.work_com_name = work_com_name;
	}

	public String getFk_people_id() {
		return fk_people_id;
	}

	public void setFk_people_id(String fk_people_id) {
		this.fk_people_id = fk_people_id;
	}

	public float getWarn_days() {
		return warn_days;
	}

	public void setWarn_days(float warn_days) {
		this.warn_days = warn_days;
	}

}