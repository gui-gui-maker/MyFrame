@charset "utf-8";
/* CSS Document */

/*==================================   全局样式  ========================================================*/

* { margin: 0; padding: 0; }
html, body { width: 100%; height: 100%; }
img {border:0; vertical-align:middle;}
body { font-size: 14px; font-family: "Microsoft Yahei", "黑体", "宋体"; overflow: hidden;background:#fff url(../images/bg.png) no-repeat center; background-size:cover; }
a {text-decoration:none;}
a:hover {text-decoration:none;}
.clear { clear: both; }


.sysmain { }




/* logo标志样式=========================== */
.m-top-logo {position:absolute; left:0; top:0; z-index:1; width:100%; height:86px; margin:0; padding:0; text-align:center;}
.m-top-logo-img {position:absolute; left:50%; top:0; z-index:1; width:800px; height:80px; padding-top:10px; margin:0 0 0 -300px; text-align:left;}
.m-top-logo-img img{ width:50px; height:50px;}
.m-top-logo-txt { font-size: 32px; font-weight: bold; color: #fff; letter-spacing: 6px; width:800px; height:80px; line-height:50px; text-align:center; margin:0 auto;}

@keyframes fadeIn {
    0% {opacity: 0; }
    50% {opacity: 0.5; }
    100% {opacity: 1; }
}

@-webkit-keyframes fadeIn {
    0% {opacity: 0; }
    50% {opacity: 0.5; }
    100% {opacity: 1; }
}

.fadeIn { animation-name: fadeIn;
      animation: fadeIn 3s  ;
      -webkit-animation: fadeIn 3s ;
      -moz-animation: fadeIn 3s ; }
	  
	  
