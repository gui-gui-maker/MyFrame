/* 新增 最小化 向下还原 关闭 按钮 2014-10-08 ============================================================*/
.windows-btn {position:absolute;top:0px;right:0px;z-index:100;height:18px;background-image:url("images/window/m-top-btna1.png");background-position:0px 0px;background-repeat:no-repeat;}
.windows-small {float:left;width:28px;text-indent:-999em;}
.windows-large {float:left;width:28px;text-indent:-999em;}
.windows-close {float:left;width:50px;text-indent:-999em;}
.windows-btn.windows-smallover {background-position:0px -18px;}
.windows-btn.windows-largeover {background-position:0px -36px;}
.windows-btn.windows-closeover {background-position:0px -54px;}
.windows-btn.restore {background-position:0px -72px;}
.windows-btn.restore.windows-smallover {background-position:0px -90px;}
.windows-btn.restore.windows-largeover {background-position:0px -108px;}
.windows-btn.restore.windows-closeover {background-position:0px -126px;}