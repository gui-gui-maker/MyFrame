/*-------------------------------------图片新闻*/
.mod_focus_show {float:left; width:279px; height:288px; position:relative; overflow:hidden; border:#bfbcaf solid 1px; margin-right:9px;}
.mod_focus_pic { display:block; width:279px; height:288px;}
.mod_focus_pic li, .mod_focus_pic a, .mod_focus_pic img { display: block; width:279px; height:209px; }
.mod_focus_pic li {position: absolute; left: 0; top: 0; }

.mod_focus_title { position: absolute; z-index: 1; left: 0; bottom:50px; overflow: hidden; zoom: 1; width:279px; height:29px; border-bottom:#bfbcaf solid 1px;}
.mod_focus_title li { width:259px; overflow: hidden; word-break: keep-all; word-wrap: keep-all; white-space: nowrap; table-layout: fixed; -o-text-overflow: ellipsis; text-overflow: ellipsis; padding-left:8px;}
.mod_focus_title li h3 {font-size:12px; line-height:29px; _line-height:29px; margin:0; padding:0; font-weight:normal;}

.mod_focus_list {overflow: hidden; height:48px; border-bottom:#bfbcaf solid 1px; border-top:#d9d9d9 solid 1px; position:absolute; left:0; bottom:0;}
.mod_focus_list li {width: 69px; height:48px; float:left; margin: 0 px 0 px; position: relative; *display: inline;}
.mod_focus_list li a { display: block; width:48px; height:32px; padding:8px 6px; }
.mod_focus_list li img { display: block; width:48px; height:32px; }
.mod_focus_list li a:hover { _background: none; }
.mod_focus_list li a:hover .mask, .mod_focus_list .current .mask { display: none; }
.mod_focus_list li .mask { display: block; height: 50px; width:48px; position: relative; top: -50px; margin: 0 0 -50px; eft: 0; bottom: 0; background: #000; opacity: .5; filter: alpha(opacity=50); }
.mod_focus_list li .border { display: none; }
.mod_focus_list .current .border { display: block; height: 57px; width: 60px; overflow: hidden; position: absolute; left: 0; bottom: 0; background:url(../images/hoverbj.png) no-repeat left center; z-index:-1; }
