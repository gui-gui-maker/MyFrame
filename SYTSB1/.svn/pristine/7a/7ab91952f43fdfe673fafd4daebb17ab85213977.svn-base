/*公示页面内容*/
.publicity {overflow:hidden;}
.publicctn {border-left:#bfbcaf solid 1px; margin-left:12px; padding-bottom:25px;}
.pubbox {padding:25px 0px 5px 0px; font-size:14px;}
.pubbox h2 {margin:0; padding:0; font-size:22px; font-family:"微软雅黑", "宋体"; font-weight:normal; height:42px; background:url(../images/publich2bj.png) no-repeat right top; line-height:42px; padding-left:30px; color:#c00; position:relative;}
.pubbox .pubicon {position:absolute; left:-10px; top:10px; width:24px; height:24px; background:url(../images/sound.png) no-repeat center; _background:url(../images/sound.gif) no-repeat center;}
.pubbox .pubmore {position:absolute; right:10px; top:0px; font-size:14px;}
.pubbox ul {padding:8px 0px 0px 30px; display:block; overflow:hidden;}
.pubbox ul li {text-align:right; color:#999; height:32px; line-height:32px; background:url(../images/point.png) no-repeat left center; padding-left:25px; border-bottom:#ccc dashed 1px; font-size:12px;}
.pubbox ul li a {font-size:14px;}
.pubbox ul li span {float:left; width:830px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; text-align:left;}
.pubbox table {border-collapse:collapse; margin:20px 0px 0px 30px;}
.pubbox table th {height:46px; font-weight:bold; line-height:18px; background:#f27a14; text-align:center; color:#fff; border:#fff solid 1px;}
.pubbox table td {border:#dcdcdc solid 1px; padding:5px 0px;}


