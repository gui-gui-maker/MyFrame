.headline p {font:12px "宋体"; overflow:hidden; _zoom:1;}
.headline p span a {background:url(../images/pointer.png) no-repeat left center; padding-left:13px; margin-right:5px; display:inline-block; float:left; width:300px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis;}
