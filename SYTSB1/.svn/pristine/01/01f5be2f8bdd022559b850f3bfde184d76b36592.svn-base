.l-page-title {position:relative;width:100%;height:60px;background:url("../images/basic/l-page-title.png") #ffffff repeat-x 0px -60px;border-bottom:1px solid #cacaca;}
.l-page-title-div {position:absolute;right:0px;top:0px;width:590px;height:60px;background:url("../images/basic/l-page-title.png") no-repeat right 0px;}
.l-page-title.has-icon .l-page-title-text {left:50px;display:block;}
.l-page-title.has-icon .l-page-title-note {left:50px;}
.l-page-title.has-note .l-page-title-text {top:10px;}
.l-page-title.has-icon .l-page-title-icon {display:block;}
.l-page-title.has-note .l-page-title-note {display:block;}
.l-page-title-text {position:absolute;left:10px;top:20px;}
.l-page-title-text h1 {padding:0px;margin:0px;font-size:16px;color:#004f9c;}
.l-page-title-note {display:none;position:absolute;left:10px;top:35px;color:#687081;line-height:16px;height:16px;overflow:hidden;margin:0px 10px 0px 0px;}
.l-page-title-note a,.l-page-title-note a:link,.l-page-title-note a:visited {color:#325A99;text-decoration:none;margin-right:5px;}
.l-page-title-note a:hover {color:#FF0000;text-decoration:underline;}
.l-page-title-icon {display:none;position:absolute;width:32px;height:32px;left:10px;top:50%;margin-top:-16px;}
.l-page-note {padding:5px;}
.l-page-note-div {padding:5px;color:#9b8e6c;border-radius:5px 5px 5px 5px;border:1px solid #ebc384;background:#fffbf0;}
.l-page-title-button {position:absolute;top:50%;margin-top:-15px;left:20px;}
.l-page-title-button a,.l-page-title-button a:link,.l-page-title-button a:visited {border:1px solid #007fbd;background:#e0f6ff;color:#003464;padding:5px 10px;margin-right:5px;text-decoration:none;display:inline-block;}
.l-page-title-button a:hover {border:1px solid #ad9835;background:#fff2b7;color:#534400;text-decoration:none;}
.l-page-title-button .jt1 {background:url("../images/basic/nav-process/jt1.png") no-repeat 0px 0px;display:inline-block;hegiht:25px;width:15px;border:1px solid #FF0000;}
.l-page-title-lct ul {margin:0px;padding:0px;list-style-type:none;}
.l-page-title-lct ul li {display:inline-block;padding-right:15px;background:url("../images/basic/nav-process/jt1.png") no-repeat right center;display:inline-block;margin:0px 0px 8px 0px;}
.l-page-title-lct ul li div {}
.l-page-title-lct a,.l-page-title-lct a:link,.l-page-title-lct a:visited {border:1px solid #ba7e4d;background:#ffdcc0;color:#6f3100;padding:5px 10px;margin-right:5px;text-decoration:none;display:block;box-shadow:3px 3px 5px 0px #8E8E8E;filter:progid:DXImageTransform.Microsoft.shadow(direction=135,color=#8E8E8E,strength=5);border-radius:5px 5px 5px 5px;cursor:pointer;}
.l-page-title-lct a:hover {border:1px solid #9cbd00;background:#e3ffe0;color:#534400;text-decoration:none;}
.l-page-title-lct .lct-is-no {background:none;}
.lct-is-disabled a,.lct-is-disabled a:visited,.lct-is-disabled a:link,.lct-is-disabled a:hover {border:1px solid #9a9a9a;background:#f3f3f3;color:#9a9a9a;cursor:default;}



