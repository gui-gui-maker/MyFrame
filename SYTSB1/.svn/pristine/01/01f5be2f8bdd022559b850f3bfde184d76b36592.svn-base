/* 系统左边菜单 accordtion结构 */
.arrowlistmenu{width:94%;overflow:hidden;margin:0px auto;cursor:default;}
div.menuheader a{color:#03509e;}
.arrowlistmenu div{margin:4px 0 4px 0;padding:0;}
.arrowlistmenu .menuheader{color:#2d71b8; font-weight:bold;background:white url("../images/basic/menu/titlebar.jpg") repeat-x center left;padding:7px 0 5px 10px;cursor:hand;cursor:pointer;border:solid 1px #b7cce7;height:16px!important;position:relative;text-indent:20px;}
.arrowlistmenu .menu1-icon {display:block;width:20px;height:24px;position:absolute;left:5px;top:-2px;border:0px solid #FF0000;}/*菜单前面的图标*/
.arrowlistmenu .menu1-text {margin:0px;}/*菜单一级文字*/
.arrowlistmenu .openheader{ background: url("../images/basic/menu/titlebar-active.jpg");color:white;height:16px!important;border:1px solid #008eda!important;font-weight:bold;}
.arrowlistmenu ul{ list-style-type: none;margin:0;padding:0px 5px 5px 5px;margin-bottom:8px; background-color:white; border:#90c0ff solid 1px;}
.arrowlistmenu ul li{padding-bottom:0px;overflow:hidden;position:relative;}
.arrowlistmenu ul li .text_slice {background-position:0px center;background-repeat:no-repeat;}
.arrowlistmenu ul li a{background-color:white;color:#000000;display: block;text-decoration: none;border-bottom: 1px solid #dadada;width:100%;}
.arrowlistmenu ul li a:hover{ background: url("../images/basic/menu/titlebar_itemBg.jpg") no-repeat 10px center !important; color:black;text-decoration:none;}
.arrowlistmenu ul li li{font-weight:normal;padding-left:5px;}
.arrowlistmenu ul li ul{font-weight:normal;border:0px solid #FF0000;}
.arrowlistmenuCurrent{background: url("../images/basic/menu/titlebar_itemBg.jpg") no-repeat 10px center !important; }
/*.arrowlistmenu ul li span{background: url("../images/basic/menu/arrowbullet.png") no-repeat;background-position:3px 4px; display: block;padding-top:3px;padding-bottom:3px;padding-left: 19px; }
*/
.arrowlistmenu .hasChildren span{background: url("../images/basic/menu/nolines_plus.gif") no-repeat 3px center; display: block;padding:8px 0px 8px 22px;}
.arrowlistmenu .noChildren span{background: url("../images/basic/menu/arrowbullet.jpg") no-repeat 3px center; display: block;padding:8px 0px 8px 22px;}
.arrowlistmenu .spanParentStyle{background: url("../images/basic/menu/nolines_plus.gif") no-repeat 3px center; display: block;padding:8px 0px 8px 22px;}
.arrowlistmenu .spanStyle{background: url("../images/basic/menu/arrowbullet.jpg") no-repeat 3px center !important; display: block;padding:8px 0px 8px 22px;}
