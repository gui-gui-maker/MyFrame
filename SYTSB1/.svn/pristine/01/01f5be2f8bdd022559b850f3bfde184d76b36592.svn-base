textarea {overflow:auto;width:100%;}
.show {display:block;}
.hide,.hidden {display:none;}

.l-t-td-left {text-align:right;width:100px;white-space:nowrap;padding:5px 5px 5px 5px;border:0px solid #CFE3F8;border-bottom:0px;border-right:0px;}
