.n_l_box_list {width:100%;overflow: hidden;float: left; text-align:left;}

.n_l_box_list ul, .n_l_box_list li { margin:0; padding:0;}
.n_l_box_list li {overflow:hidden; white-space:nowrap; text-overflow:ellipsis; position:relative; padding-left:20px;font-size:14px; color:#333; background:url(../images/in_top_icon.png) no-repeat left center; list-style:none; border-bottom: 1px dashed #eee;}
.n_l_box_list li span { float:right;}
.n_l_box_list li a,.n_l_box_list li a:link {color:#333;}
.n_l_box_list li .n_l_box_list_bt a:hover{color:#f00;}
.n_l_box_list_bt {width:70%; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; position:relative; float:left; line-height:30px; *line-height:28px; }
.n_l_box_time{float:right; font-size:12px; color:#bbb; line-height:30px; *line-height:28px; }


