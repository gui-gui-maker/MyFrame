.table-list1 ul li > div:nth-child(4) {border:0px solid #FF9966;width:150px;}
.table-list1 ul li > div:nth-child(5) {border:0px solid #FF9966;width:150px;}

.table-list1 ul li:nth-child(2n) { background: rgba(255,255,255,.1);}

.ta-list-head td:nth-child(1) {border:0px solid #0000FF;width:50px;}
.ta-list-head td:nth-child(2) {border:0px solid #990099;}
.ta-list-head td:nth-child(3) {border:0px solid #CCCC99;width:80px;}
.ta-list-head td:nth-child(4) {border:0px solid #FF9966;width:80px;}



