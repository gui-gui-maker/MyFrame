@charset "utf-8";
/* CSS Document */

/*==================================   全局样式  ========================================================*/

* { margin: 0; padding: 0; }
html, body { width: 100%; height: 100%; }
img {border:0; vertical-align:middle;}
body { font-size: 14px; font-family: "Microsoft Yahei", "黑体", "宋体"; overflow: hidden;background:#fff url(../images/bg02.jpg) no-repeat center; background-size:cover; }
a {text-decoration:none;}
a:hover{text-decoration:none;}
.clear { clear: both; }
.hha, .hha:active, .hha:visited { display: inline-block; color: White;  text-decoration: none; -moz-border-radius-bottomright: 4px; -moz-border-radius-bottomleft: 4px; -webkit-border-radius: 4px 4px 4px 4px; border-radius: 4px 4px 4px 4px; }
.search_bar { float: right; transition: width 0.3s;}
.input{border: none;outline: none; }
.search_ico {line-height:36px;}
/*html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td {background: none;	margin: 0;	padding: 0;	outline: 0px;	border: 0px currentColor;	border-image: none;	font-size: 100%;	vertical-align: baseline;}
article, aside, details, figcaption, figure, header, hgroup, footer, menu, nav, section {display: block;}*/
ol, ul { list-style: none; }
blockquote, q { quotes: none; }
blockquote::before { content: none; }
blockquote::after { content: none; }
:focus { outline: 0px; }
ins, del { text-decoration: none; }
table { border-collapse: collapse; border-spacing: 0; }
.fl{float:left;}
.fr{float:right;}
.tl{ text-align: left!important;  }
.tr{ text-align: right!important;  }
.clear{clear:both;display:block;overflow:hidden;height:0;line-height:0;}
.mrt10{ margin-top: 10px;  }
.pd20{padding:20px;}
.justify {text-align:justify;text-justify:distribute-all-lines;/*ie6-8*/text-align-last:justify;/* ie9*/-moz-text-align-last:justify;/*ff*/-webkit-text-align-last:justify;/*chrome 20+*/}
.toe{word-break:keep-all;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}/*超出省略号*/


/*transition*/
.tran200{-webkit-transition:all 0.2s linear; -moz-transition:all 0.2s linear; -ms-transition:all 0.2s linear; transition:all 0.2s linear;}
.tran300{-webkit-transition:all 0.3s linear; -moz-transition:all 0.3s linear; -ms-transition:all 0.3s linear; transition:all 0.3s linear;}
.tran400{-webkit-transition:all 0.4s linear; -moz-transition:all 0.4s linear; -ms-transition:all 0.4s linear; transition:all 0.4s linear;}
.tran500{-webkit-transition:all 0.5s linear; -moz-transition:all 0.5s linear; -ms-transition:all 0.5s linear; transition:all 0.5s linear;}
.tran1000{-webkit-transition:all 1s linear; -moz-transition:all 1s linear; -ms-transition:all 1s linear; transition:all 1s linear;}

.scaleimg:hover{ -webkit-transform: scale(1.1); -moz-transform: scale(1.1); -ms-transform: scale(1.1); transform: scale(1.1);}
.rotateY:hover{ -webkit-transform: rotateY(180deg); -moz-transform: rotateY(180deg); -ms-transform: rotateY(180deg); transform: rotateY(180deg); }
.translateX a:hover{-webkit-transform:translateX(6px); -moz-transform: translateX(6px); -ms-transform:translateX(6px); transform:translateX(6px);}




/* logo标志样式=========================== */
.m-top-logo {position:absolute; left:0px; bottom:0; z-index:1; width:100%; height:65px;  margin:0; padding:0; text-align:center; font-size: 60px; color: #fff; text-shadow: 0px 2px 5px rgba(0, 0, 0, 0.13); }
.m-top-logo-img img{ width:50px; height:50px;}
.m-top-logo-txt { font-size: 32px; font-weight: bold; color: #fff; letter-spacing: 6px; height:50px; line-height:50px; text-align:center; margin:0 auto;}

@keyframes fadeIn {
    0% {opacity: 0; }
    50% {opacity: 0.5; }
    100% {opacity: 1; }
}

@-webkit-keyframes fadeIn {
    0% {opacity: 0; }
    50% {opacity: 0.5; }
    100% {opacity: 1; }
}

.fadeIn { animation-name: fadeIn;
      animation: fadeIn 3s  ;
      -webkit-animation: fadeIn 3s ;
      -moz-animation: fadeIn 3s ; }
	  
.top_ser_bg{ height:141px; background: url(../images/tit_bg.png); /*background:rgba(255,255,255,0.2);*/}


/*顶部 右边功能=======================================*/
.m-top-upfun {position:absolute; right:20px; top:35px; z-index:5; font-size: 18px; color: #fff; }
.m-top-msg {position:relative; float:left; line-height:36px; padding-right:12px; margin-right:15px;}
.m-top-msg a {color:#333;}
.m-top-msg span {position:absolute; top:0; right:0; width:16px; height:16px; font-size:11px; font-family:Arial; line-height:16px; text-align:center; color:#fff; background:#f00; border-radius:8px;}

.m-top-user {position:relative; float:left; width:auto; line-height:24px; zoom:1; }
.m-top-user a{ color:#999;}
.m-top-user a:hover{color:#fff;}
.m-user-login{ background:url(../images/login_icon.png) no-repeat; width:24px; height:24px;float:left; margin-right:10px; }
.m-top-user a:hover .m-user-login { background:url(../images/login_icon.png) 0 -24px no-repeat;transition: all 0.3s;}


.m-user-photo {float:left; width:36px; height:36px; margin-right:10px; background:#fff; background-position:top center; background-size:cover; overflow:hidden; border-radius:18px;}
.m-user-photo img {width:32px; height:32px; margin:2px;}
.m-user-name { float:left;}
.m-user-xdnaw {position:absolute; top:14px; right:0; width:8px; height:8px; background:url(../images/xdnaw.png) no-repeat; background-size:100%;}


.m-top-set {    float: left;width: 24px;height: 24px;margin: 6px 0;color: #fff;font-size: 24px!important;line-height: 24px;/* background: url(../images/set.png) no-repeat center; */cursor: pointer;}
.m-foot {position:absolute; bottom:0; left:0; width:100%; font-size:12px; text-align:center; color:#999;background: url(../images/foot_bg.png); background: rgba(0,0,0,.5); height:58px; line-height:58px;}




/*中间主要内容区================================================================*/
.m-center2 {position:absolute; left:2%;right:1.5%; bottom:90px; top: 110px; }
.m-center-wrap { height: 100%;  }
.m-center-wrap .transition {transition: margin-top .5s ease,width .5s ease;}
.m-area-top  { text-align:center; position: relative; height: 100px; color: #fff;   }
.m-a-tit {font-size: 60px;line-height: 100px;   }

.m-top-l-img{ position:absolute; left: 0; top:0; height: 100px; width:50%; background: url(../images/t_l_bg.png) no-repeat left; background-size: contain; }
.m-top-r-img{ position:absolute; right: 0; top:0; height: 100px; width:50%; background: url(../images/t_r_bg.png) no-repeat right;background-size: contain; }

.half { height: 50%; position: relative; /*overflow: hidden; */ }


.col1 { width:300px; height: 400px; float: left; background:#1b99e6 url(../images/col_max_bg.jpg) no-repeat left bottom;  }
.col2 { width:860px; height: 400px; float: left;  /*overflow: hidden;*/}

.tsboxcont  a{ box-shadow:4px 5px 0px 0px rgba(0, 0, 0, 0.07); color: #fff; position: relative; }
.tsboxcont  .tit { font-size: 30px; padding:20px 0 0 20px;  }
.tsboxcont  .numb { font-size: 72px; padding-left: 40px; padding-top: 10px;  }
.tsboxcont  .numb span { font-size: 30px;   }

.col1 a { display:block; height: 100%; height: 100%; background: url(../images/ico01.png) no-repeat right bottom; }
.col2 a { display: block; width:400px; height:185px; float: left; margin-left: 30px;margin-bottom: 30px;}
.col2 a.ico2 { background:#5562ca url(../images/ico02.png) no-repeat right bottom;} 
.col2 a.ico3 { background:#ea8010 url(../images/ico03.png) no-repeat right bottom;} 
.col2 a.ico4 { background:#ab67b4 url(../images/ico04.png) no-repeat right bottom;} 
.col2 a.ico5 { background:#00bb9c url(../images/ico05.png) no-repeat right bottom;} 

.tsboxcont  a:hover { }
.tsboxcont  a:hover .numb { text-shadow: 0px 2px 5px rgba(0, 0, 0, 0.13);   }




.tsboxcont .border_top{
  position:absolute;height:1px;width:0;font-size:0; background:rgba(255,255,255,.8);top:0;left:0;
  -webkit-transition:all 0.5s ease-out;
  -o-transition:all 0.5s ease-out;
  transition:all 0.5s ease-out;
}
.tsboxcont  .border_right{
  position:absolute;height:0px;width:1px;font-size:0;background:rgba(255,255,255,.8);bottom:0;right:0;
  -webkit-transition:all 0.5s ease-out;
  -o-transition:all 0.5s ease-out;
  transition:all 0.5s ease-out;
}
.tsboxcont  .border_bottom{
  position:absolute;height:1px;width:0px;font-size:0;background:rgba(255,255,255,.8);right:0;bottom:0;
  -webkit-transition:all 0.5s ease-out;
  -o-transition:all 0.5s ease-out;
  transition:all 0.5s ease-out;
}
.tsboxcont  .border_left{
  position:absolute;height:0px;width:1px;font-size:0;background:rgba(255,255,255,.8);left:0;top:0;
  -webkit-transition:all 0.5s ease-out;
  -o-transition:all 0.5s ease-out;
  transition:all 0.5s ease-out;
}

.tsboxcont  a:hover  .border_top , .tsboxcont  a:hover  .border_bottom {width:100%; }
.tsboxcont  a:hover  .border_left , .tsboxcont  a:hover  .border_right {height:100%; }






@-webkit-keyframes rolling {
    from {
        transform:translate3d(0,0px,0);
    }
    to {
       
        transform:translate3d(0,-10px,0);
    }
}



