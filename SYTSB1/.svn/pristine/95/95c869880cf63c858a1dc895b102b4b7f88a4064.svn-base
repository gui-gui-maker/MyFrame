.process2 {margin:0; padding:5px;list-style:none;/*font-size:12px;*/ font-weight:bold; text-align:center; overflow:hidden; /*background:url(../images/arrow0.png) no-repeat center top;*/ margin:3px 0px 0px; *padding-left:120px;}
.process2 li {*float:left; display:inline-block; /*width:123px;*/ text-align:center; background:url(../images/basic/nav-process/arrow1.png) no-repeat center top; padding:12px 10px 0 10px; *padding:12px 2px 0 2px; overflow:hidden;cursor:pointer;min-width:130px;width:auto !important;width:130px;/*IE6最小宽度*/}
.process2 li a {margin-top:15px; display:inline-block; color:#000; text-decoration:none;}
.process2 li.active { background:url(../images/basic/nav-process/arrow2.png) no-repeat center top; padding-top:12px; font-weight:bold; }
.process2 li:hover { background:url(../images/basic/nav-process/arrow2.png) no-repeat center top; padding-top:12px; font-weight:bold; color:#951d22; }
.process2 li.active a { color:#e6640d; }