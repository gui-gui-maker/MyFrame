	0% {-webkit-transform: translateY(-100%); transform: translateY(-100%);/* height:0;*/}
	100% {-webkit-transform: translateY(0); transform: translateY(0); /*height:640px;*/}
}

@-webkit-keyframes slit {
	50% {-webkit-transform: translateZ(-250px) rotateY(89deg); opacity: .5;}
	100% {-webkit-transform: scale(1); -moz-transform: scale(1); -ms-transform: scale(1); transform: scale(1); opacity: 1;}
}
@-moz-keyframes slit {
	50% { -moz-transform: translateZ(-250px) rotateY(89deg); opacity: .5;}
	100% { -webkit-transform: scale(1);	-moz-transform: scale(1); -ms-transform: scale(1); transform: scale(1); opacity: 1;}
}
@keyframes slit {
	50% { transform: translateZ(-250px) rotateY(89deg); opacity: .5;}
	100% {-webkit-transform: scale(1); -moz-transform: scale(1); -ms-transform: scale(1); transform: scale(1); opacity: 1;}
}




