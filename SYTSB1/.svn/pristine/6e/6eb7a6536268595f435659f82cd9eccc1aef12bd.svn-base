.classification3{width: 100%;height: auto;}
.classification3 .yj3{width: 100%;height: 32px;border-bottom: 1px solid #d2d2d2;font-size: 14px;color: #1e1e1e;line-height: 32px;cursor: pointer;}
.classification3 .yj3 img{vertical-align: middle;margin: 0 6px 0 20px;}
.yjnr3{width: 100%;height: auto;border-bottom: 1px solid #d2d2d2;padding-bottom: 50px;}
.yjnr3 ul li{width: 63px;height: 58px;float: left;margin-left: 10px;cursor: pointer;margin-top: 10px;text-align: center;padding-top: 5px;}
.yjnr3 ul li p{font-size: 12px;color: #1e1e1e;}
.yjnr3 ul li:hover{background: url("../images/gjBg_03.png") no-repeat left 2px;;}

.right .attribute{width: 100%;height: auto;border-bottom: 1px solid #b9b9b9;padding-bottom: 2px;}
.right .attribute p{width: 100%;height: 34px;background: #d8d8d8;font-size: 14px;color: #1e1e1e;line-height: 34px;padding-left: 10px;}
.right .attribute input{width: 54px;height: 25px;font-size: 12px;color: #1e1e1e;}
.right .attribute span{width: 37px;text-align:right; display: inline-block;margin: 20px 5px 0 5px;font-size: 14px;color: #1e1e1e;}
/* .right .attribute .xzhou,.right .attribute .yzhou{margin: 20px 10px 0 16px;} */

.right .assembly{width: 100%; height: auto;}
.right .assembly .ss{width: 100%;height: 45px;background: #d8d8d8;border-top: 1px solid #b9b9b9;border-bottom:1px solid #b9b9b9; }
.ss p{width: 78px;height: 100%;text-align: center;line-height: 45px;color: #1e1e1e;font-size: 14px;float: left;}
.ss input{width: 128px;height: 30px;float: left;border: 0;background: url("../images/sss_03.png") no-repeat left center;padding-left: 25px;margin-top: 8px;}

.dianjileft{width: 5px;height: 80px;position: absolute;left: 232px;top: 40%;cursor: pointer;z-index: 20;}
.dianjiright{width: 5px;height: 80px;position: absolute;right: 232px;top: 40%;cursor: pointer;z-index: 20;}
.dianjileft:hover , .dianjiright:hover {opacity: 0.4;}

.zhong{position: absolute;left: 0;top: 0;z-index: 10;width: 100%; height: 100%;}

