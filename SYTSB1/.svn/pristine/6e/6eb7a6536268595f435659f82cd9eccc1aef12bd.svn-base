
html,body {overflow:hidden;margin:0;padding:0;width:100%;height:100%;}

#web{width: 100%;height: 100%; position: absolute; left: 0; top: 0; z-index: 3;}

#header{width: 100%;height: 54px;background: #2e3946; position: absolute; left: 0; top: 0;}
.open{width: 76px; text-align: center;line-height: 54px;color: #9db1c5;font-size: 14px;float: left;}
/*.open:hover{background: #438cf6;color: #fff;}*/

.toolbar{width: 84.58%;height: 100%;float: left; /*overflow: hidden;*/}
.toolbar ul li{float: left;/*width: 28px;*/height: 54px;text-align: center; position: relative; margin: 0 6px;}
.toolbar ul li a {display: inline-block;width: 28px;height: 28px;margin-top: 13px;cursor: default;}
.toolbar ul li a img{vertical-align: middle;margin-top: 5px;cursor: pointer;}
.toolbar ul .shu{background: url("../images/tabshu_02.png") no-repeat right top; padding-right: 13px;}
.toolbar ul li a:hover{background: #000; border-radius: 3px;}
.toolbar ul li.disabled a {pointer-events: none; opacity: 0.2;}

.toolbar ul .liu{width: 50px!important;}
.toolbar ul .liu a{width: 50px!important;height: 28px;}

.active a{background: #438cf6;border-radius: 5px;}

.out{padding-left: 20px; height: 28px; position: absolute; right:20px; top:13px; background: #2e3946;}
.out img{vertical-align: middle; line-height: 28px;}
.out .shez{font-size: 14px;color: #9db1c5; padding-left: 22px; padding-right: 22px; background: url(../images/out1_03.png) no-repeat left center; margin-right:8px;}
.out a{display: inline-block; padding:0 5px; line-height: 28px; float: left;}
.out a:hover {background: #000; border-radius: 3px;}

#content{width: 100%; position: absolute; left: 0; right: 0; bottom: 0; top: 54px;z-index: 3;}

.left {width:237px; border-right: 5px solid #b9b9b9; position: absolute;left: 0;top: 0; bottom: 0; z-index: 10; overflow-x:hidden; overflow-y:scroll; display: block;}

.right {width:237px; border-left: 5px solid #b9b9b9; position: absolute;right: 0;top: 0; bottom: 0; z-index: 10; overflow-x:hidden; overflow-y:scroll; display: block;}

.center {position:absolute; left:237px; right: 237px; top: 0; bottom: 0; z-index: 10; min-width: 100px; overflow:hidden;}

/*定义滚动条高宽及背景
 高宽分别对应横竖滚动条的尺寸*/
::-webkit-scrollbar {width:2px;height:10px;background-color:#F5F5F5;}

.search{width: 100%;height: 46px;background: #d8d8d8;}
.searchleft select{float: left;width: 65px;height: 34px;font-size: 12px;color: #717171;margin-left: 10px;margin-top: 5px;padding-left: 5px;}
.searchright input{float: left;width: 96px;height: 28px;font-size: 12px;color: #717171;background: url("../images/fdj_03.png") no-repeat 5px center;padding-left: 22%;margin-left: 5px;margin-top: 2px;border:0;outline:none;}
.searchright{background: #fff;width: 123px;height: 32px;float: right;border: 1px solid #999;margin-right: 10px;margin-top: 5px;}

.classification{width: 100%;height: auto;}
.classification .yj{width: 100%;height: 32px;border-bottom: 1px solid #d2d2d2;font-size: 14px;color: #1e1e1e;line-height: 32px;cursor: pointer;}
.classification .yj img{vertical-align: middle;margin: 0 6px 0 20px;}
.yjnr{width: 100%;height: auto;border-bottom: 1px solid #d2d2d2;padding-bottom: 50px;}
.yjnr ul li{width: 63px;height: 58px;float: left;margin-left: 10px;cursor: pointer;margin-top: 10px;text-align: center;padding-top: 5px;}
.yjnr ul li p{font-size: 12px;color: #1e1e1e;}
.yjnr ul li:hover{background: url("../images/gjBg_03.png") no-repeat left 2px;}

.classification1{width: 100%;height: auto;}
.classification1 .yj1{width: 100%;height: 32px;border-bottom: 1px solid #d2d2d2;font-size: 14px;color: #1e1e1e;line-height: 32px;cursor: pointer;}
.classification1 .yj1 img{vertical-align: middle;margin: 0 6px 0 20px;}
.yjnr1{width: 100%;height: auto;border-bottom: 1px solid #d2d2d2;padding-bottom: 50px;}
.yjnr1 ul li{width: 63px;height: 58px;float: left;margin-left: 10px;cursor: default;margin-top: 10px;text-align: center;padding-top: 5px; border:1px solid #fff;}
.yjnr1 ul li p{font-size: 12px;color: #1e1e1e;}
.yjnr1 ul li:hover{background:#ccebf8; border:1px solid #199ed8;}

.classification2{width: 100%;height: auto;}
