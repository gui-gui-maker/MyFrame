/*弹出框样式*/
.tanchu{width: 200px;height: 200px;border: 3px solid #eee;position: absolute;left: 50%;margin-left: -100px;top: 50%;margin-top: -100px;z-index: 30;display: none;}
.mune{width: 80px;height: 98px;border: 1px solid #000;margin-top: 12px;margin-left: 10px;position: absolute;left: -18px;top: 42px;z-index: 40;display: none;}
.mune li{width: 100%!important;height: 25px!important;line-height: 40px;}
.xi div{width: 80%;border-bottom: 1px solid #666;margin: 0 auto;margin-top: 12px;}
.zong div{width: 80%;border-bottom: 2px solid #666;margin: 0 auto;margin-top: 12px;}
.cu div{width: 80%;border-bottom: 3px solid #666;margin: 0 auto;margin-top: 12px;}
.dacu div{width: 80%;border-bottom: 4px solid #666;margin: 0 auto;margin-top: 12px;}
.mune li:hover{background: rgba(67,140,246,0.5)}
.mune li:hover div{border-color: #fff;}

.active {background:#ccebf8!important; border:1px solid #199ed8!important;}

/*组件*/
.assembly .assemblylist{width: 100%;height: auto;}
.assembly .assemblylist .assemblyname{width: 100%;height: 20px;color: #1e1e1e;font-size: 12px;background: url("../images/page_03.png") no-repeat left center;padding-left: 20px;margin: 10px 0 0 10px;line-height: 20px;cursor: pointer;}
.assembly .assemblylist ul li a{color: #1e1e1e;font-size: 12px;display: inline-block;}
.assembly .assemblylist ul{margin-top: 10px;}
#assembly1 a{background: url("../images/page1_03.png") no-repeat left center;padding-left: 25px;}
#assembly2 a{background: url("../images/page2_03.png") no-repeat left center;padding-left: 25px;}
#assembly3 a{background: url("../images/page3_03.png") no-repeat left center;padding-left: 25px;}
.assembly .assemblylist ul li{padding-left: 35px;width: 100%;height: 30px;line-height: 30px;cursor: pointer;}
.activeTi{background: #ccebf8;}
.listT ul li img{float: right;margin-right: 20px;margin-top: 8px;}


/*color*/
.setcc{ width: 30px;height: 30px; margin:0 auto; position: relative; }
.setcc img { position: absolute; bottom: 2px; right: 2px; z-index: 10; }



.aui-about-dow { opacity: 0; visibility: hidden; position: absolute; left: -50px; top: 110%; min-width: 170px; padding: 8px 10px; background: #fff; border-radius: 2px; z-index: 99; box-shadow: 0 2px 6px rgba(0, 0, 0, 0.3); -webkit-transition: all 0.25s ease-in-out; -moz-transition: all 0.25s ease-in-out; -o-transition: all 0.25s ease-in-out; }
.drop-down:hover .aui-about-dow { top: 100%;  opacity: 1; visibility: visible; -webkit-transition: all 0.25s ease-in-out; -moz-transition: all 0.25s ease-in-out; -o-transition: all 0.25s ease-in-out; filter: alpha(opacity=100);  }
.toolbar .aui-about-dow li a { padding: 0; line-height: inherit; display: block; padding: 8px 0 8px 15px; color: #313131; font-size: 16px; -webkit-transition: all 0.3s ease; -moz-transition: all 0.3s ease; -ms-transition: all 0.3s ease; -o-transition: all 0.3s ease; transition: all 0.3s ease; font-size: 14px; color: #616161; width: auto!important; margin-top: 0; height: auto; padding:5 0; }
.toolbar .aui-about-dow li a:hover { color: #9DB1C5; }
.toolbar .aui-about-dow li { width: 100%; margin:0; height: auto; }




