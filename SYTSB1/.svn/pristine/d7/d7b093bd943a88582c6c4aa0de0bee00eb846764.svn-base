#product {width:100%;position:relative;margin:0 auto;}
#content { position:relative;  margin:0px auto; width:94%; height:150px; /*display:inline-block;  */ overflow:hidden; }
#content_list { position:relative;margin:0 auto; left:0px;}
#product .prev{display:none; cursor:pointer; width:20px; height:150px; position:absolute;top:0px;left:0px;display:none;}
#product .prev:hover {background:url(images/welcome-desktop/btn-bg.png) no-repeat left center;}
#product .prev:active {}
#product .prev span {width:100%;height:100%; background:url(images/welcome-desktop/btn02.gif) no-repeat center center;display:block;}
#product .next{display:none; cursor:pointer; width:20px; height:150px; position:absolute;top:0px;right:0px;display:none;}
#product .next:hover {background:url(images/welcome-desktop/btn-bg.png) no-repeat right center;}
#product .next:active {}
#product .next span {width:100%;height:100%; background:url(images/welcome-desktop/btn01.gif) no-repeat center center;display:block;}
#product.hover {}
#product.hover .prev {display:block;}
#product.hover .next {display:block;}
/*20130927图标左右滚动样式================================ end */

