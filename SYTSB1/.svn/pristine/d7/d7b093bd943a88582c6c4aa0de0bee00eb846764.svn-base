.worktips {width:48%; overflow:hidden; float:left;}
.notice {width:48%; overflow:hidden; float:right;}
.wklist {overflow:hidden;}
.wklist ul, .wklist li {margin:0; padding:0; list-style:none;}
.wklist li {background:url(images/welcome-desktop/arrow.png) no-repeat left 11px; padding:5px 0px 5px 18px; color:#999; height:14px; overflow:hidden; position:relative;}
.wklist li a {color:#363636;}
.wklist li a:hover {color:#f00;}
.wklist li div {white-space:nowrap; text-overflow:ellipsis; overflow:hidden; margin-right:50px;}
.wklist li span.wttm {position:absolute; top:5px; right:0;}


.footer{position:absolute;z-index: 18;bottom:0px;width:100%;text-align: center;height:50px;overflow:visible;}
.footer div { margin-top:8px; -webkit-margin-top:0px; width: 113px; height: 35px; display: inline-block; background-repeat:no-repeat;}
.footer .footer-left {background:url(images/welcome-desktop/dsk_footer-left.png) no-repeat; margin-right:-4px;}
.footer .footer-right {background:url(images/welcome-desktop/dsk_footer-right.png) no-repeat; margin-left: -4px;}
#dot-list {margin-top:8px;display:inline-block;height:35px;vertical-align:top;/*-webkit-padding-start: 0;-webkit-margin-after: 0;-webkit-margin-before: 0;*/ background:url(images/welcome-desktop/dsk_dot-listbj.png) repeat-x;}
#dot-list .selected{background-image:url(images/welcome-desktop/dsk_dot-listbj3.png) !important; background-repeat:repeat-x; background-position:bottom;}
.dot {display: inline-block;cursor: pointer;width: 90px;outline: none;text-align: center;height: 34px;position: relative;}
.dot:not(.selected):hover{background-image:url(images/welcome-desktop/dsk_dot-listbj3.png) !important; background-repeat:repeat-x; background-position:bottom;}
.dot:not(.selected):active{background-image:url(images/welcome-desktop/dsk_dot-listbj3.png) !important; background-repeat:repeat-x; background-position:bottom;}
.dot div {background-color:transparent; cursor:inherit;  width:100%; -webkit-appearance:caret; -webkit-transition:all 0.2s; text-align:center; margin-top:7px; border:0; color:#444;}
.dot div:focus {  cursor: auto;}



