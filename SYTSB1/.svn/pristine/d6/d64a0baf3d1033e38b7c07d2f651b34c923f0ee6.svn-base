/*======================== 网站管理--生成管理 ===============================*/
.generate {margin:3% auto 0;}
.generate td {/*border:#ddd solid 1px; */padding:15px 22px; border-collapse:collapse;}
.gnrt_txt {font:14px "宋体"; margin-top:10px; width:500px;}
.gnrt_txt th , .gnrt_txt td {border:#ddd solid 1px; line-height:22px; border-collapse:collapse; padding:7px 20px;}
.gnrt_txt th {text-align:right; font-weight:bold; font-size:12px;}

