package com.lsts.webservice.cxf.server.report;

/**
 * 报告书信息数据传输对象
 * @ClassName ReportDTO
 * @JDK 1.7
 * @author GaoYa
 * @date 2014-09-16 上午09:35:00
 */
public class ReportDTO {
	private String device_type;	// 设备类型
	private String com_name;	// 使用单位
	private String report_sn;	// 报告书编号
	private String check_type;	// 检验类别（检验性质）
	private String check_date;	// 检验日期
	private String check_status;// 检验状态
	private Double advance_fees	;//	预收金额
	
	public ReportDTO() {
		super();
	}

	public ReportDTO(String device_type, String com_name, String report_sn,
			String check_type, String check_date, String check_status, Double advance_fees) {
		super();
		this.device_type = device_type;
		this.com_name = com_name;
		this.report_sn = report_sn;
		this.check_type = check_type;
		this.check_date = check_date;
		this.check_status = check_status;
		this.advance_fees = advance_fees;
	}
	
	public String getDevice_type() {
		return device_type;
	}
	public void setDevice_type(String device_type) {
		this.device_type = device_type;
	}
	public String getCom_name() {
		return com_name;
	}
	public void setCom_name(String com_name) {
		this.com_name = com_name;
	}
	public String getReport_sn() {
		return report_sn;
	}
	public void setReport_sn(String report_sn) {
		this.report_sn = report_sn;
	}
	public String getCheck_type() {
		return check_type;
	}
	public void setCheck_type(String check_type) {
		this.check_type = check_type;
	}
	public String getCheck_date() {
		return check_date;
	}
	public void setCheck_date(String check_date) {
		this.check_date = check_date;
	}
	public String getCheck_status() {
		return check_status;
	}
	public void setCheck_status(String check_status) {
		this.check_status = check_status;
	}

	public Double getAdvance_fees() {
		return advance_fees;
	}

	public void setAdvance_fees(Double advance_fees) {
		this.advance_fees = advance_fees;
	}
}
