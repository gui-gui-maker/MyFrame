@charset "utf-8";
/* CSS Document */


/*==================================   全局样式  ========================================================*/

/** {-moz-box-sizing:border-box; -ms-box-sizing:border-box; box-sizing:border-box;}*/
html, body {width: 100%; height:100%;}
/*img {border:0; vertical-align:middle;}*/
/*body {font-size:14px; font-family: "Microsoft Yahei", "黑体", "宋体"; overflow: hidden; color:#fff; background:#000 url(../img/timg.jpg) no-repeat;}*/
em {font-style:normal;}
a {text-decoration:none; outline:none;}
* :hover , * :active , * :focus {outline:none;}
.clear {clear:both; *display:inline;/*IE only*/}

.page_main {position:relative; z-index: 999;-moz-box-sizing:border-box; -ms-box-sizing:border-box; box-sizing:border-box;}

.side_right {position:fixed; right:0; top:0; width:300px; color:#fff;z-index: 99;}

/* 区块样式 */
.m-a-boxes {position:relative; /*width:440px; height:300px; height:31%;*/ width:100%; margin-bottom:5px; z-index:102; zoom:1; overflow:hidden;}
.m-a-beij {position:absolute; left:0; top:0; z-index:103; width:100%; height:100%; box-shadow:0 0 25px rgba(255,255,255,0.5) inset; background:rgba(8,75,137,0.75); background:#084b89\9; opacity:0.75\0; filter:alpha(opacity=75)\0; filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=75)\0;}
.m-a-border {position:absolute; left:0; top:0; z-index:104; /*width:436px; height:296px; height:100%;*/width:296px; border:2px dashed #fff; opacity:0.2; filter:alpha(opacity=20);}

.m-a-boxes-tit {position:relative; z-index:105; padding:0 20px; height:40px; /* width:100%;margin-bottom:15px;*/ line-height:40px; color:#fff; background:url(../img/mboxbgline.png) no-repeat center bottom; background-size:100%; overflow:hidden;cursor: pointer;}
.m-a-boxes-tit span {position:absolute; right:20px; top:0px; z-index: 999999999; cursor: pointer;}
.m-a-main {position:absolute; z-index:106; width:300px; padding:0 10px;top:48px;bottom:0;border:0px solid #FF0000;}
.m-a-main img {}

.column1 {height:142px;} /*142*/
.column1 .m-a-border {height:138px;} /*138*/
.column2 {height:35px;}
.column2 .m-a-border {height:315px;}
.column3 {height:35px;}
.column3 .m-a-border {height:174px;}
.column4 {height:200px;}
.column4 .m-a-border {height:196px;}
.column5 {height:200px;}
.column5 .m-a-border {height:196px;}
.column6 {height:35px;}
.column6 .m-a-border {height:196px;}
.column7 {height:35px;}
.column7 .m-a-border {height:196px;}
.column8 {height:35px;}
.column8 .m-a-border {height:196px;}

.mapset {}
.mapset dl , .mapset dt , .mapset dd {margin:0; padding:0;}
.mapset dl {display:block; float:left; width:290px; line-height:22px; margin-bottom:7px;}
.mapset dt {float:left; width:70px; text-align:right; margin-right:2px;}
.mapset dd {float:left;}
#sjxsfw{width: 198px;}

.mapset select {width:72px; height:22px; line-height:22px\9; border:1px solid #ccc; background:#fff;border-radius: 3px;}
.mapset select option {width: 72px;}
.mapset input {height:22px; line-height:22px\9; border:1px solid #ccc; background:#fff;border-radius: 3px;}

.chart-panel {width:100%;height:100%;}

.list-panel {}
.list-panel ul {list-style:none; margin:0; padding:0; height: 139px; width: 320px; overflow-y: auto!important;}
.list-panel li {list-style:none; margin:0; padding:0; white-space: nowrap;width:248px; text-overflow:ellipsis; overflow: hidden;}
.list-panel a {color:#fff;}
.list-panel li {background:url(../img/icondm.png) no-repeat left top; line-height:16px; padding-left:25px; margin:0px 0 10px!important; cursor:pointer;}
.list-panel li:hover a , .list-panel li.active a {color:#ff0;}
.list-panel li:hover , .list-panel li.active  {background:url(../img/icondm.png) no-repeat left -16px;}

.list-panel div {position:absolute;margin-top: 0; margin-left: 0;}

.timewp {position:relative;}
.timewp a.icontm {position:absolute; right:4px; top:5px; width:18px; height:18px; background:url(../img/icontm.png) no-repeat; border:1px solid transparent;}
.timewp a.icontm:hover {border:1px solid #f00;}

.picView{display: none; width: 60%; height: 80%; position: fixed; top: 10%; left: 15%; background-color: #084b89; z-index: 99; border-radius: 5px; box-shadow: 3px 3px 10px #000; padding: 2px; text-align: center;}
.chartView{display: none; width: 75%; height: 90%; position: fixed; top: 5%; left: 8%; background-color: #084b89; z-index: 99; border-radius: 5px; box-shadow: 3px 3px 10px #000; padding: 2px; text-align: center;opacity:0.8}
.chartLabels{width:98%;height:6%;border-bottom:1px solid #ccc;margin-top:2%;padding-left:2%}
.chartLabel{width:10%;height:98%;border:1px solid #ccc;background-color:#0080ff;left:5%;top:5%;margin-left:1%;float:left;font-family:'微软雅黑';line-height:40px;text-align: center; border-top-left-radius:10px;border-top-right-radius:10px;-moz-border-top-left-radius:10px;-moz-border-top-right-radius:10px;-webkit-border-top-left-radius: 10px;-webkit-border-top-right-radius: 10px;-o-border-top-left-radius: 10px;-o-border-top-right-radius: 10px; cursor: pointer;}
.picClose{ position: absolute;right: 10px; top:10px; width: 10px; height: 10px; padding: 10px; background: url(../img/iw_close.gif) no-repeat 10px center; z-index: 999999; cursor: pointer;}
.picView img{max-width: 100%;}
.pano_close{right: 310px!important;}
.BMap_scaleTxt{color:#00cf88!important;}
/*.BMapLabel{background-color:rgba(255,255,255,0)!important;border:0px!important;z-index: 99999999!important;}*/

.bootstrap-switch{height:20px!important;min-width:70px!important;}
.bootstrap-switch-handle-on {height:20px!important; padding-top:1px!important;}
.bootstrap-switch-label {height:20px!important; padding-top:1px!important;}
.bootstrap-switch-handle-off{height:20px!important; padding-top:1px!important;}

.beginDiv00{position:absolute; top:0px; z-index: 8888; width: 100%; height: 100%; background-color:rgba(255,255,255,0.9);}
.beginDiv{position:absolute; top:0px; z-index: 9999; width: 100%; height: 100%; text-align: center; /*background: url(../img/sc.jpg) no-repeat; background-size:cover;*/}
/*.beginImg{height: 100%; cursor: pointer;}*/

.shrinkRight{padding-left:4px;position:fixed; right:300px;top:0;width:11px; z-index:9; /*background: rgba(46,146,236,0.4);*/ 
background:-webkit-linear-gradient(to right, rgba(46,146,236,0), rgba(46,146,236,0.4));
background:-moz-linear-gradient(to right, rgba(46,146,236,0), rgba(46,146,236,0.4));
background:-o-linear-gradient(to right, rgba(46,146,236,0), rgba(46,146,236,0.4));
background:-webkit-gradient(linear, 0 0, 100% 0, from(rgba(46,146,236,0)), to(rgba(46,146,236,0.4)));
background:linear-gradient(to right, rgba(46,146,236,0), rgba(46,146,236,0.4));opacity: 0; /*transition:auto 0.5s;*/}
.shrinkRight a{display:block;background:url(../img/sbg.png) no-repeat -33px center; width: 11px; z-index: 99; position: relative;}
.shrinkRight:hover{opacity: 1;}
