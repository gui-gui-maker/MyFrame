}
@-moz-keyframes typing {
    0% { background-color: rgba(0, 0, 0, 1); box-shadow: 12px 0px 0px 0px rgba(0, 0, 0, 0.2), 24px 0px 0px 0px rgba(0, 0, 0, 0.2); }
