/* 统计区块样式 */
.m-area-left {position:absolute; top:50px;bottom:50px;left:1%; z-index:101; width:30%;}
.m-area-right {position:absolute; top:50px;bottom:50px;right:1%; z-index:101; width:66%;}

.m-area-top { position:absolute; left:32%; right:32%; top:10px;z-index:99; color:#fff;   }
.m-area-bottom {position:absolute; left:32%; right:32%; bottom:20px;  height: 20%; z-index:99; color:#fff; margin-bottom: 20px; }

.m-area-left .m-a-boxes {
	position:relative; 
	width:100%; 
	height:32%; 
	margin-bottom:1%; 
	z-index:102; 
	zoom:1; 
	overflow:hidden;
}
.m-area-right .m-a-boxes-big {
	position:relative; 
	width:100%; 
	height:65%; 
	z-index:102; 
	zoom:1; 
	overflow:hidden;
}
.m-area-right .m-a-boxes1{
	position:relative; 
	height:32%; 
	margin-top:0.4%;
	z-index:102; 
	zoom:1; 
	overflow:hidden;
	width:48.5%;
	margin-right:1.5%;
	float:left;
}
.m-area-right .m-a-boxes2{
	position:relative; 
	height:32%; 
	margin-top:0.4%;
	z-index:102; 
	zoom:1; 
	overflow:hidden;
	width:48.5%;
	margin-left:1.5%;
	float:left;
}
.m-a-beij {position:absolute; left:0; top:0; z-index:103; width:100%; height:100%; box-shadow:0 0 25px #008aff inset; opacity:0.3; filter:alpha(opacity=20);}
.m-a-border {position:absolute; left:0; top:0; z-index:104; /*width:436px; height:296px;*/width:100%; height:100%; border:1px solid #1d5ed7; opacity:0.6; filter:alpha(opacity=60);}
.m-a-pl1{ position:absolute; left:0; top:0;z-index:105; width: 4px; height: 4px; background:#32a1ee;  }
.m-a-pl2{ position:absolute; left:0; bottom:0;z-index:105; width: 4px; height: 4px; background:#32a1ee;  }
.m-a-pr1{ position:absolute; right:0; top:0;z-index:105; width: 4px; height: 4px; background:#32a1ee;  }
.m-a-pr2{ position:absolute; right:0; bottom:0;z-index:105; width: 4px; height: 4px; background:#32a1ee;  }

.m-a-boxes-tit {position:relative; z-index:105; margin-top: 10px; padding:0 20px 0 10px; height:40px; margin-bottom:3%; font-size:16px; line-height:40px; text-align:center; color:#fff; overflow:hidden; border-radius: 0 20px 20px 0; border:1px solid #1d5ed7; display: inline-block; border-left: 0}
.no-title .m-a-boxes-tit {display:none;}
.no-title .m-a-main {top:0; background:url(../images/mboxbgline.png) no-repeat center 40px;}

.m-area-bottom .m-a-boxes-tit { background: none; border: 0; height:initial;  margin:0; border-radius: 0; padding: 0; line-height: normal; }

.m-top-l-img{ position:absolute; left: 0; top:20px; height: 8px; width: 30%; background: url(../images/max_t_left.png) no-repeat left; background-size: contain; }
.m-top-r-img{ position:absolute; right: 0; top:20px; height: 8px; width: 30%; background: url(../images/max_t_right.png) no-repeat right;background-size: contain; }
.m-area-top  .m-a-tit { text-align: center; font-size: 36px; line-height: 40px;  }
.c-start { 
	text-align: right; 
	overflow: hidden; 
	position:absolute;
	top:5px;
	left:100%;
	
}
.c-end { 
	text-align: right; 
	overflow: hidden; 
	position:absolute;
	top:5px;
	left:130%;
}
.date_year{ float: left; width: 150px; height:40px; position: relative; text-align: center; font-size: 22px; line-height: 40px;  }
.date_year .m-a-beij {   box-shadow:0 0 25px #1f1f1f inset; opacity:0.3; filter:alpha(opacity=30);}
.date_year .m-a-pl1,
.date_year .m-a-pl2,
.date_year .m-a-pr2,
.date_year .m-a-pr1{  width: 2px; height: 2px;   }
.date_year:hover { cursor: pointer; }
.date_year:hover .m-a-beij{ box-shadow:0 0 40px #008aff inset; opacity:0.6; filter:alpha(opacity=60);  }
.date_money { float: right; font-size: 14px;  }
.today_data{ padding:4px 0; font-size:30px; font-family:"Times New Roman", Times, serif; font-weight:bold; letter-spacing:1px; text-shadow:-1px 0 #001d76,0 1px #001d76,1px 0 #001d76,0 -1px #001d76,0 0 0.1em #00d2ff, -0 -0 0.1em #00d2ff; color:#ff7826; }

