.table-list1 {font-size:12px;}
.table-list1 {}
.table-list1 ul {margin:0px;padding:0px;list-style-type:none;_display:inline-block;/*ie6*/overflow:hidden;display:table;border-collapse:collapse;width:100%}
.table-list1 ul li {clear:both;color:#FFFFFF;cursor:pointer;display:table-row;padding:5px;}
.table-list1 ul li:hover {background:rgba(26,80,188,0.6);;border:0px solid #0000FF;}
.table-list1 ul li > div {display:table-cell;padding:5px;}
.table-list1 ul li > div:nth-child(1) {border:0px solid #0000FF;width:80px;}
.table-list1 ul li > div:nth-child(2) {border:0px solid #990099;text-align:left;}
.table-list1 ul li > div:nth-child(3) {border:0px solid #CCCC99;width:90px;}
.table-list1 ul li > div:nth-child(4) {border:0px solid #FF9966;width:60px;}
.ta-list-head td:nth-child(1) {border:0px solid #0000FF;width:80px;}
.ta-list-head td:nth-child(2) {border:0px solid #990099;}
.ta-list-head td:nth-child(3) {border:0px solid #CCCC99;width:90px;}
.ta-list-head td:nth-child(4) {border:0px solid #FF9966;width:60px;}
#demohq_b ul li > div:nth-child(1) {border:0px solid #0000FF;width:80px;}
#demohq_b ul li > div:nth-child(2) {border:0px solid #990099;text-align:left;width:130px;}
#demohq_b ul li > div:nth-child(3) {border:0px solid #CCCC99;width:90px;}
#demohq_b ul li > div:nth-child(4) {border:0px solid #FF9966;width:60px;}

