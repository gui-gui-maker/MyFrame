.c-data-select {position:absolute;height:50px;top:0px;left:50%;margin-left:400px;background:url("../images/icon-112.png") no-repeat left center;padding-left:30px;z-index:118;}
.c-data-select-text {color:#ffffff;font-size:16px;padding:10px 0px 10px 0;width:100px;text-align:center;cursor:pointer;float:left;}
.c-data-select-text .caret{display:inline-block;width:0;height:0;margin-left:2px;vertical-align:middle;border-top:4px dashed;border-right:4px solid transparent;border-left:4px solid transparent;margin-left:6px;}
.c-data-select-defult {color:#ffffff;font-size:16px;padding:10px 0px 10px 0;/*width:100px;*/text-align:center;cursor:pointer;float:left;}
.c-data-select:hover .c-data-select-time {display:block;}
.c-data-select:hover .c-data-select-text {background-color:#0d224c;border:1px solid #9bdae9;border-bottom:0;position:relative;z-index:1;}
.c-data-select-time {display:none;background:#0d224c;border:1px solid #9bdae9;border-top:0;width:100px;box-shadow:0px 0px 15px 5px #17c3e9;padding-top:50px;}
.c-data-select-time ul {margin:0px;padding:0px;list-style-type:none;}
.c-data-select-time ul li {}
.c-data-select-time ul li:hover {}
.c-data-select-time ul li div {text-align:center;}
.c-data-select-time ul li div a,.c-data-select-time ul li div a:link,.c-data-select-time ul li div a:visited {color:#ffffff;text-decoration:none;padding:5px;display:block;}
.c-data-select-time ul li div a:hover,.c-data-select-time ul li.hover {color:#ffffff;text-decoration:none;background:#17c3e9;}
.c-data-select-time ul li div a span {}


