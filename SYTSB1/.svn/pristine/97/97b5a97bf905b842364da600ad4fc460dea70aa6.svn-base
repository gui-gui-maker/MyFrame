html,body {padding:0;margin:0; background:#fff;}

.bartxt {text-shadow:none !important;font-weight:normal;}

.ui-header {position:absolute;height:170px;top:0;left:0;right:0;border:0px solid #221aff;}
.ui-content {position:absolute;top:170px;bottom:0;left:0;right:0;overflow:auto;border:0px solid #FF0000;}
.ui-loader {display:none;}

.a-index {padding:0;width:100%;height:100%;position:relative;}
.a-index .bg {position:absolute;width:100%;height:120px;background-image:url("images/app-ytmz/a-index2.jpg");background-size:cover;}
.a-index .logo {position:absolute;width:100%;height:100px;background-image:url("images/app-ytmz/logo.png");background-size:72px;background-repeat:no-repeat;background-position:15px 24px;}
.a-index .user {position:absolute; z-index:1; left:105px; top:17px; color:#fff; line-height:250%;text-shadow:none !important;font-weight:normal;}
.a-index .user h1 {margin:0; padding:0; font-size:2em; font-weight:normal;}
.a-index .user h2 {margin:0; padding:0; font-size:1.7em; font-weight:normal;}
.a-index .user p {margin:0; padding:0; font-size:1.5em;}

.a-index .messages {/*position:absolute;*/width:100%;z-index:2;height:60px; line-height:60px;/*top:170px;*/ background:#ffe674; overflow:hidden;}
.a-index .db {width:30%;position:absolute;top:0;left:0; margin:0 10%;}
.a-index .xx {width:30%;position:absolute;top:0;right:0; margin:0 10%;}
.a-index .text {float:left; text-align:center;font-size:2em; padding-right:5%;}
.a-index .no {float:left; text-align:center;font-size:1.5em;color:#ff0000; padding-top:10px; position:relative;}
.a-index .no img {width:36px;}
.a-index .no span {position:absolute; right:-20px; top:0px; display:block; width:32px; height:26px; line-height:26px; color:#fff; background:url(images/app-ytmz/mes_tipbj.png) no-repeat; background-size:32px 26px;}

.a-guide-menu {}
.a-index .bar {/*position:absolute;width:100%;z-index:2;*/ padding:5px; background:#fff; /*overflow:hidden;*/clear:both;text-align:left;}
.bar01 {border-bottom:1px solid #e5e5e5; height:120px;}
.bar02 {}
.a-index .bar02 item {margin-bottom:20px;}
.a-index .bar .item {display:inline-block; margin:2px auto; width:32%; text-align:center;color:#858585; border:1px solid transparent;}
/*.a-index .bar .item:hover {background:#f3faff; border:1px solid #01a0fe; border-radius:8px;}*/
/*.a-index .bar .item:active {background:#fff7b1; border:1px solid #fead41;}*/
.a-index .bar .item .a {color:#858585;padding:10px;}
.a-index .bar .item .a:hover {text-decoration:none;}
.a-index .bar .item .bartxt {line-height:40px;height:40px;overflow:hidden;border:0px solid #FF0000; white-space:nowrap;text-overflow:ellipsis;font-size:1.5em;}
.a-index .bar .item .icon {width:72px; height:40px; text-align:center; padding:16px 0px; border-radius:50%; margin:0 auto;}
.a-index .bar .item .icon img {width:40px;/*text-shadow:#000055;*/}

