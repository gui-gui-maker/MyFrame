.a-index .content {/*position:absolute;z-index:2; top:370px;*/width:100%; padding-top:10px;}
.a-index .content .wrap {margin:0 3%;}
.a-index .content .wrap .table {margin:0 auto; border-collapse:collapse;}
.a-index .content .wrap .table td {border-bottom:1px solid #c8c8c8;}
.a-index .content .wrap .table .a {width:25%; font-size:2em; text-align:right; padding:10px 10px; color:#343434;}
.a-index .content .wrap .table .b {width:55%;font-size:1.5em; line-height:150%; color:#666;}
.a-index .content .wrap .table .n {padding:0px 5px;color:#ff0000;}
.a-index .content .wrap .table .c {width:55%;font-size:1.5em; line-height:150%; color:#666; padding:10px 0px;}

.ui-content .a-menu-loading {padding:15px;}
.a-menu-nav {float:left;width:100%;line-height:1.5em;background:#d9e8f9;font-size:1.1em;border-bottom:1px solid #0097fe;}
.a-menu-nav .d-big {float:left;}
.a-menu-nav .d {float:left;}
.a-menu-nav .a,.a-menu-nav .a:link,.a-menu-nav .a:visited {color:#325A99;text-decoration:none;display:block;border-right:1px solid #0097fe;padding:10px;}
.a-menu-nav .a:hover {}
.a-menu-nav .a:action {background:#f9f1d0;}


@media handheld, only screen and (max-width:400px){
	.a-index .bg {height:120px;}
	.a-index .logo {height:100px; background-size:72px; background-position:15px 24px;}
	.a-index .user {left:100px; top:17px; line-height:250%;}
	.a-index .user h1 {font-size:2em;}
	.a-index .user h2 {font-size:1.7em;}
	.a-index .user p {font-size:1.5em;}

	.a-index .messages {top:120px;}
	.a-index .db {width:38%; margin:0 6%;}
	.a-index .xx {width:38%; margin:0 6%;}
	.a-index .text {font-size:1.5em;}
	.a-index .no {font-size:1.2em;}
	.a-index .no span {top:5px;}

	.bar01 {}
	.bar02 {}
	.a-index .bar .item {/*width:32.5%;*/ /*font-size:1.5em;*/}
	.a-index .bar .item .bartxt {line-height:220%;}

	.a-index .content {}
	.a-index .content .wrap {margin:0 2%;}
	.a-index .content .wrap .table .a {width:27%; font-size:1.4em; padding:10px 10px 10px 0px;}
	.a-index .content .wrap .table .b {width:55%;font-size:1.2em; line-height:150%;}
	.a-index .content .wrap .table .n {padding:0px 2px;}

	.ui-header {height:120px;}
	.ui-content {top:120px;}
}

@media handheld, only screen and (min-width:400px) and (max-width:1024px){
	.a-index .bg {height:145px;}
	.a-index .logo {height:120px; background-size:96px; background-position:15px 24px;}
	.a-index .user {left:130px; top:20px; line-height:300%;}
	.a-index .user h1 {font-size:2.4em;}
	.a-index .user h2 {font-size:2em;}
	.a-index .user p {font-size:1.6em;}

	.a-index .messages {/*position:absolute;*/width:100%;z-index:2;height:60px; line-height:60px;/*top:170px;*/ background:#ffe674; overflow:hidden;}
	.a-index .bar {/*position:absolute;width:100%;z-index:2;*/ padding:20px 0px 10px; background:#fff; /*overflow:hidden;*/}
	
	.ui-header {height:120px;}
	.ui-content {top:145px;}
}

@media handheld, only screen and (min-width:1024px){
	.a-index .bg {height:180px;}
	.a-index .logo {height:160px; background-size:128px; background-position:15px 20px;}
	.a-index .user {left:170px; top:20px; line-height:370%;}
	.a-index .user h1 {font-size:3em;}
	.a-index .user h2 {font-size:2.5em;}
	.a-index .user p {font-size:2em;}
}