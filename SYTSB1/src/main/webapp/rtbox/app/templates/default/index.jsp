<script type="text/javascript">
	$(function() {
		$("form").ligerForm();
		initForm();

	});

	function initForm() {
		comm.mask("正在获取数据，请稍后...");

		$.ajax({
			url : 'com/rt/deal/detailMap.do',
			type : 'post',
			dataType : "json",
			data : {
				fk_report_id : "${param.fk_report_id}"
			},
			success : function(response) {
				if (response.success) {
					var data = response.data;
					comm.LoadEnd();
					if (data != null) {
						for ( var row in data) {
							var name = data[row]["name"];
							var value = data[row]["value"];
							setInputVal(name, value);
						}
					}

				} else {
					comm.Content("获取失败！" + response.msg);
					comm.LoadEnd(2);
				}

			}
		});
		return false;
	}

	function submitForm() {
		comm.mask("正在保存，请稍后...");
		var array = new Array();
		$("input").each(function() {
			var name = $(this).attr("name");
			var value = getInputVal(name);
			var json = {
				name : name,
				value : value
			}
			array.push(json);
		});

		$.ajax({
			url : 'com/rt/deal/saveMap.do',
			type : 'post',
			dataType : "json",
			contentType : 'application/json;charset=utf-8', //设置请求头信息  
			data : JSON.stringify(array),
			success : function(response) {
				if (response.success) {
					comm.Content("保存成功！");
				} else {
					comm.Content("保存失败！" + response.msg);
				}
				comm.LoadEnd(3);
			}
		});
		return false;
	}
</script>
