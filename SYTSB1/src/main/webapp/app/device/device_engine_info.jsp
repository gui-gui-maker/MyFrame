<%@ page contentType="text/html;charset=UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html>
<head>
</head>

<body>
  
       <table border="1" cellpadding="3" cellspacing="0" width="" class="l-detail-table">
       
       	<input name="id" type="hidden" />
		
	
       <tr>
        <td class="l-t-td-left">牌照号码：</td>
        <td class="l-t-td-right">
        <input name="p_pzhm" type="text" ltype='text' validate="{required:false,maxlength:20}"/>
        </td>
        <td class="l-t-td-left">发动机编号：</td>
        <td class="l-t-td-right">
        <input name="p_fdjxh" type="text" ltype='text' validate="{required:false,maxlength:20}"/>
        </td>
       </tr>
       <tr>
        <td class="l-t-td-left">出厂(底盘)编号：</td>
        <td class="l-t-td-right">
        <input name="p_djccbh" type="text" ltype='text' validate="{required:false,maxlength:20}"/>
        </td>
        <td class="l-t-td-left">额定载重量：</td>
        <td class="l-t-td-right">
        <input name="p_edzzl" type="text" ltype='text' ligerui="{suffix:'kg'}" validate="{required:false,maxlength:20}"/>
        </td>
       </tr>
       <tr>
        <td class="l-t-td-left">运行速度：</td>
        <td class="l-t-td-right">
        <input name="p_yxsd" type="text" ltype='text' ligerui="{suffixWidth:'40',suffix:'km/h'}" validate="{required:false,maxlength:20}"/>
        </td>
        <td class="l-t-td-left">动力方式：</td>
        <td class="l-t-td-right">
        <u:combo name="p_dlfs" code="BASE_PARA_P_DLFS" /> 
        </td>
       </tr>
       <tr>
        <td class="l-t-td-left">驾驶室定员：</td>
        <td class="l-t-td-right">
        <input name="p_jssdy" type="text" ltype='text' validate="{required:false,maxlength:20}"/>
        </td>
        <td class="l-t-td-left">燃油类型：</td>
        <td class="l-t-td-right">
        <u:combo name="p_rylx" code="BASE_PARA_P_RYLX" /> 
        </td>
       </tr>
       <tr>
        <td class="l-t-td-left">厂牌型号：</td>
        <td class="l-t-td-right">
        <input name="p_cpxh" type="text" ltype='text' validate="{required:false,maxlength:20}"/>
        </td>
        <td class="l-t-td-left">车辆类型：</td>
        <td class="l-t-td-right">
        <input name="p_cllx" type="text" ltype='text' validate="{required:false,maxlength:20}"/>
        </td>
       </tr>
       <tr>
        <td class="l-t-td-left">空车重量：</td>
        <td class="l-t-td-right">
        <input name="cp_by1" type="text" ltype='text' ligerui="{suffix:'kg'}" validate="{required:false,maxlength:20}"/>
        </td>
        <td class="l-t-td-left">颜色：</td>
        <td class="l-t-td-right">
        <input name="cp_by2" type="text" ltype='text' validate="{required:false,maxlength:20}"/>
        </td>
       </tr>
    
 
      </table>
   






</body>
</html>
